# SMS 提供商選擇
# 設定為 '360' 或 'infobip'
SMS_PROVIDER=360

# 360 SMS 配置
# 從 360 平台獲取這些值
API_KEY_360=your_360_api_key
API_SECRET_360=your_360_api_secret
# 參考文件連結（非實際發送端點）
# 實際發送採用 https://sms.360.my/gw/bulk360/v3_0/send.php
API_BASE_URL_360=https://sms.360.my/developers/v3.0

# Infobip SMS 配置（備用）
# 從 https://www.infobip.com/signin 獲取這些值

# Infobip API Key
INFOBIP_API_KEY=your_infobip_api_key_here

# Infobip API Base URL
INFOBIP_API_BASE_URL=your_base_url.api.infobip.com

# Infobip 發送號碼（Sender Number 或 Alphanumeric Sender ID，如 'MyBazaar'）
INFOBIP_SENDER_NUMBER=MyBazaar
